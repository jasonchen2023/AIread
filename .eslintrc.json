{
  "extends": "airbnb",
  "env": {
      "browser": true,
      "es6": true
  },
  "parserOptions": {
	"ecmaVersion": 2020
},
  "rules": {
      "strict": 0,
      "quotes": [2, "single"],
      "no-else-return": 0,
	  "no-alert": "off",
      "new-cap": ["error", {"capIsNewExceptions": ["Router"]}],
      "no-console": 0,
      "import/no-unresolved": [2, { "caseSensitive": false } ],
      "no-unused-vars": ["warn", { "vars": "all", "args": "none" }],
      "no-underscore-dangle": 0,
      "arrow-body-style": 0,
      "one-var": ["error", { "uninitialized": "always", "initialized": "never" }],
      "one-var-declaration-per-line": ["error", "initializations"],
      "max-len": ["error", 200],
      "no-extra-parens": 0,
      "no-restricted-syntax": [0, "DebuggerStatement"],
      "no-debugger": "warn",
      "no-unreachable": "warn",
			"object-curly-newline": ["error", {
        "ObjectExpression": { "multiline": true },
        "ObjectPattern": { "multiline": true }, // idk about these, just using for import declaration
        "ImportDeclaration": "never", // from https://eslint.org/docs/latest/rules/object-curly-newline
        "ExportDeclaration": { "multiline": true, "minProperties": 3 }
    	}],

      "react/jsx-uses-react": 2,
			"react/jsx-uses-vars": 2,
			"react/react-in-jsx-scope": 2,
			"react/prop-types": 0,
			"react/destructuring-assignment": 0,
			"react/jsx-first-prop-new-line": 0,
			"react/jsx-filename-extension": 0,
      "react/jsx-no-bind": 0,
			"jsx-a11y/click-events-have-key-events": 0,
			"jsx-a11y/no-noninteractive-element-interactions": 0,
			"react/jsx-one-expression-per-line":0,
      "jsx-a11y/control-has-associated-label":0,
      "jsx-a11y/no-static-element-interactions": 0
	},
	"plugins": [
			"react"
	]
}